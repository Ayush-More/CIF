{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/care/enrollment/[careType]/availability/page.jsx",
    "src/app/care/enrollment/[careType]/page.jsx",
    "src/app/care/enrollment/[careType]/personal-details/page.jsx",
    "src/app/profile/[user]/page.tsx",
    "src/app/components/ReviewSection.jsx",
    "src/app/services/chat.js",
    "src/lib/socket.js",
    "server.js"
  ],
  "exclude": [
    "node_modules"
  ]
}